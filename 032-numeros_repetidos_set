lista_true = [1,2,3,4,5]
lista_false = [1,2,2,3,4]

if len(lista_true) == len(set(lista_true)):
    print("A lista: ", lista_true , "não tem elementos repetidos")
else:
    print("A lista: ", lista_true ,  "tem elementos repetidos, verifique")


if len(lista_false) == len(set(lista_false)):
    print("A lista: ", lista_false ," não tem elementos repetidos")
else:
    print("A lista: ", lista_false ," tem elementos repetidos, verifique")